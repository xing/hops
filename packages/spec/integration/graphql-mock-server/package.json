{
  "name": "fixture-graphql-mock-server",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "10 || 12 || 13 || 14"
  },
  "hops": {
    "gracePeriod": 0,
    "allowServerSideDataFetching": true,
    "port": "8029",
    "graphqlUri": "http://localhost:8029/graphql",
    "graphqlMockSchemaFile": "<rootDir>/mock-schema.js",
    "mixins": ["."]
  },
  "jest": {
    "testEnvironment": "../../helpers/env.js",
    "setupFilesAfterEnv": [
      "../../jest.setup.js"
    ]
  },
  "scripts": {
    "start": "hops start",
    "build": "hops build"
  },
  "dependencies": {
    "hops": "*",
    "hops-graphql": "*"
  }
}
